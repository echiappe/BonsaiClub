                  <Expression xsi:type="ipy:PythonTransform">
                    <ipy:Script>import math
from System import Tuple

@returns(Tuple[float,float,float,float,float,float,float])

def process(value):
    speed=value.Item1
    r=value.Item2
    dt=value.Item3
    l=value.Item4
    dd=speed*dt
        #adjust r
    dtheta=2*math.asin(speed*dt/(2*r))
    k=math.pi//dtheta+1
    dtheta=math.pi/k
    r=speed*dt/(2*math.sin(dtheta/2))
    #adjust L
    n=l//(speed*dt)+1
    l=n*speed*dt
    halfl=l/2
    n=float(n)
    l=float(l)
    dd=float(dd)
    halfl=float(halfl)
    return Tuple.Create(dtheta,r,n,k,l,dd,halfl)</ipy:Script>
