                    <ipy:Script>import math
from System import Tuple

@returns(Tuple[float,float])

def process(value):
    i=value.Item1
    dtheta=value.Item2
    r=value.Item3
    n=value.Item4
    k=value.Item5
    l=value.Item6
    dd=value.Item7
    i=i-1+n
    p=i%(2*(2*n+2*k))
    if p&lt;n:
        x=l/2-(p+1)*dd
        y=0
    elif p&lt;(n+k):
        turns=p-n+1
        theta=math.pi/2+turns*dtheta
        x=-l/2+r*math.cos(theta)
        y=-r+r*math.sin(theta)
    elif p&lt;(2*n+k):
        x=-l/2+(p-(n+k)+1)*dd
        y=-2*r
    elif p&lt;(2*n+2*k):
        turns=p-(2*n+k)+1
        theta=-math.pi/2+turns*dtheta
        x=l/2+r*math.cos(theta)
        y=-r+r*math.sin(theta)
    elif p&lt;2*n+2*k+n:
        x=l/2-(p-(2*n+2*k)+1)*dd
        y=0
    elif p&lt;2*n+2*k+n+k:
        turns=p-(2*n+2*k+n)+1
        theta=-math.pi/2-turns*dtheta
        x=-l/2+r*math.cos(theta)
        y=r+r*math.sin(theta)
    elif p&lt;2*n+2*k+2*n+k:
        x=-l/2+(p-(2*n+2*k+n+k)+1)*dd
        y=2*r
    else:
        turns=p-(2*n+2*k+2*n+k)+1
        theta=math.pi/2-turns*dtheta
        x=l/2+r*math.cos(theta)
        y=r+r*math.sin(theta)
    return Tuple.Create(x,float(y))</ipy:Script>
